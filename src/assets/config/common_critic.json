---
layout: compress
---

{% comment %} import scss template {% endcomment %}

{% capture data_import_scss %}
	{% include coll_ext/site-config/add-scss/critic_bootstrap_import.html %}
	{% include coll_ext/site-config/add-scss/critic_template_common_page_site_add_force_custom.html 
		current_page_site = null
	%}
{% endcapture %}

{% comment %} import js template {% endcomment %}
{% assign data_import_js = '' %}

{% assign data_import_js = '' %}
{% comment %}first, common pages site and detail{% endcomment %}
{% include coll_ext/site-config/add-html/js_common_import.html 
  folder_templates=site.data.templates.pages
  type_added_ref='common_pages'
  not_ouput_hmtl=true
%}
{% assign data_import_js = list_js_import | remove_first: delim %}
{% comment %}then common page site part{% endcomment %}  
{% include coll_ext/site-config/add-html/js_common_import.html 
  folder_templates=site.data.templates.page_site
  type_added_ref='common_page_site'
  not_ouput_hmtl=true
%}
{% assign add_list_js_import = list_js_import | remove_first: delim %}
{% assign data_import_js = data_import_js | append: add_list_js_import %}
{% if data_import_js != "" %}
	{% comment %}filter last delim{% endcomment %}
	{% assign offsetSlice = data_import_js.size | minus: delim.size %}
	{% assign data_import_js = data_import_js | slice: 0,offsetSlice %}
{% endif %}

{% comment %}json content{% endcomment %}
{
  "pages":
  [
		{
			"id": "common_critic",
		  "import_scss": "{{data_import_scss}}",
		  "import_js": "{{data_import_js}}"
		}
	],
  "config":
  {  	
  	"var_custom_style": "",
  	"delim_import_js": "{{delim}}"  	
  }
}

